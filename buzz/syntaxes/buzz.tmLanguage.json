	{
		"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
		"name": "Buzz",
		"patterns": [
			{
				"include": "#comments"
			},
			{
				"include": "#keywords"
			},
			{
				"include": "#operators"
			},
			{
				"include": "#storage"
			},
			{
				"include": "#strings"
			},
			{
				"include": "#numbers"
			},
			{
				"include": "#builtins"
			},
			{
				"include": "#libraries"
			}
		],
		"repository": {
			"comments": {
				"patterns": [{
					"name": "comment.line.number-sign.buzz",
					"begin": "#",
					"beginCaptures": {
						"1": {
							"name": "punctuation.definition.comment.documentation.buzz"
						}
					},
					"end": "\n"
				}]
			},
			"keywords": {
				"patterns": [{
					"name": "keyword.control.buzz",
					"match": "\\b(if|else|function|return|for|while|foreach|include)\\b"
				}]
			},
			"operators": {
				"patterns": [{
					"name": "keyword.operator.buzz",
					"match": "\\b(and|or|not)\\b"
				}]
			},
			"storage": {
				"patterns": [{
					"name": "storage.type.buzz",
					"match": "\\b(nil|var|id|self)\\b"
				}]
			},
			"numbers": {
				"patterns": [{
					"name": "constant.numeric.buzz",
					"match": "(^-?[0-9]+.?[0-9]*$)"
				}]
			},
			"strings": {
				"name": "string.quoted.double.buzz",
				"begin": "\"",
				"end": "\"",
				"patterns": [
					{
						"name": "constant.character.escape.buzz",
						"match": "\\\\."
					}
				]
			},
			"builtins": {
				"patterns": [{
					"name": "entity.name.function.member.buzz",
					"match": "\\b(map|reduce|size)\\b"
				}]
			},
			"libraries": {
				"patterns": [{
					"name": "support.class.member.buzz",
					"match": "\\b(swarm|stigmergy|neighbors|math|io|string)\\b"
				}]
			}
		},
		"scopeName": "source.buzz",
		"fileTypes": [".bzz"]
	}